{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Audio Visualiser",
  "version": "2021.11",
  "permissions": [
    "tabCapture",
    "activeTab",
    "alarms"
  ],
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "256": "sound.png"
    },
    "default_title": "Audio Visualizer",
    "default_popup": "popup.html"
  }
}
